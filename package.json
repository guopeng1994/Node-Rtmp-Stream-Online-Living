{
  "name": "living",
  "version": "1.0.0",
  "description": "this is an online living website whitch based on node.js",
  "config": {
    "buildDir": "web/src/output",
    "devHost": "localhost",
    "devPort": 3000,
    "devProxy": "http://localhost:80"
  },
  "scripts": {
    "build": "webpack -p --colors --progress --config ./web/webpackConfig/config",
    "prestart": "cnpm install",
    "start": "concurrently \"npm run startDev\"",
    "startDev": "node web/server ./webpackConfig/config",
    "startHttpServer": "nodemon server/app",
    "startRtmpServer": "nodemon rtmp-server/server",
    "webpack": "webpack --colors --progress --config ./web/webpackConfig/config"
  },
  "author": "guopeng",
  "license": "ISC",
